{
  "name": "grommet-swagger",
  "version": "0.1.0",
  "main": "src/index.js",
  "description": "API documentation via swagger",
  "repository": "",
  "license": "Apache-2.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production grommet pack",
    "check": "cross-env NODE_ENV=test grommet check",
    "start": "cross-env NODE_ENV=development grommet pack",
    "start-alias": "cross-env NODE_ENV=development USE_ALIAS=true grommet pack",
    "test": "grommet check -t",
    "test-update": "grommet check -t -- --updateSnapshot",
    "test-watch": "grommet check -t -- --watchAll"
  },
  "dependencies": {
    "grommet": "https://github.com/grommet/grommet/tarball/NEXT-stable",
    "grommet-icons": "^0.12.0",
    "history": "^4.7.2",
    "js-yaml": "^3.10.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "styled-components": "^2.2.3",
    "url-search-params": "^0.10.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-grommet": "^0.4.0",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "deep-assign": "^2.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-parallel": "^0.3.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "grommet-cli": "^5.1.2",
    "pre-commit": "^1.2.2",
    "react-dev-utils": "^4.2.1",
    "react-test-renderer": "^16.1.1",
    "simple-git": "^1.82.0",
    "webpack": "^3.8.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/templates/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|templates|node_modules)[/\\\\]"
    ]
  },
  "pre-commit": [
    "check"
  ]
}
